{
  "name": "iota-mam-env-vis",
  "description": "visualization of mam stream sensor",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "author": "jan",
  "engines": {
    "node": ">=8"
  },
  "repository": "https://github.com/jhab82/iota-mam-visualization",
  "main": "app.js",
  "private": true,
  "cloud-repo-tools": {
    "requiresKeyFile": true,
    "requiresProjectId": true,
    "test": {
      "app": {
        "args": [
          "app.js"
        ],
        "msg": "IOTA MAM visualization"
      }
    }
  },
  "scripts": {
    "start": "node app.js",
    "test": "repo-tools test app && ava -t 30s --tap test/*.test.js",
    "e2e": "repo-tools test deploy && E2E_TESTS=True ava -t 30s --tap test/*.test.js",
    "cover": "nyc --cache npm test; nyc report --reporter=html",
    "init-cloudsql": "node books/model-cloudsql.js"
  },
  "dependencies": {
    "@google-cloud/datastore": "2.0.0",
    "body-parser": "1.18.3",
    "express": "4.16.4",
    "iota.lib.js": "^0.5.1",
    "lodash": "4.17.11",
    "mysql": "2.16.0",
    "nconf": "0.10.0",
    "prompt": "1.0.0",
    "pug": "^2.0.3",
    "uglify-js": "3.4.9"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "^3.0.0",
    "ava": "0.25.0",
    "proxyquire": "2.1.0",
    "sinon": "^7.0.0"
  }
}
